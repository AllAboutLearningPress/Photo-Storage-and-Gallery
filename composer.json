{
    "name": "aalp/gallery",
    "type": "project",
    "description": "AALP Photo Gallery",
    "authors": [
        {
            "name": "Scotty Rippel",
            "email": "scotty@uzly.com",
            "homepage": "https://github.com/dyner",
            "role": "Project Manager"
        },
        {
            "name": "Ashraful Firoz",
            "email": "ashraf.akon201@gmail.com",
            "homepage": "https://github.com/ashrafakon",
            "role": "Backend Developer"
        },
        {
            "name": "Andrey Voltekin",
            "email": "andreyvlktn@gmail.com",
            "homepage": "https://github.com/andreyvolokitin",
            "role": "Frontend Developer"
        }
    ],
    "keywords": [
        "All About Learning Press",
        "gallery",
        "aalp-gallery"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ashrafakon/laravel-scout-elastic-aws": "*",
        "aws/aws-sdk-php": "^3.180",
        "elasticsearch/elasticsearch": "^7.13",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.5.4",
        "intervention/image": "^2.5",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.6",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^3.0",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.10",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.18",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
        {
            "url": "https://github.com/AshrafAkon/laravel-scout-elastic-aws.git",
            "type": "git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}